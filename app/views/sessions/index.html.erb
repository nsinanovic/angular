<h1>Listing sessions</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= link_to (t "show"), session %></td>
        <td><%= link_to ( t "back"), edit_session_path(session) %></td>
        <td><%= link_to ( t "destroy"), session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session', new_session_path %>
